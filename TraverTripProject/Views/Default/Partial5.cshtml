@using TraverTripProject.Models.Class
@model List<Blog>

<div class="box1-left">

    @foreach (var item in Model)
    {
        <div class="bot-img">
            <img src="@item.BlogImg" alt="" />
        </div>
        <div class="bot-desc">
            <p>@item.Description.Substring(0, 70) ...</p>
        </div>
        <div class="clear"></div>
    }
</div>
